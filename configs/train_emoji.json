{
    "data_root": "data/emoji",
    "source_list": "data/emoji/source_list.json",
    "style_name": "emoji",
    "learning_rate": 1e-5,
    "step_milestones": [20, 100],
    "train_epochs": 200,
    "batch_size": 8,
    "freeze_unet": false,
    "freeze_vae": true,
    "training_timestep_range": [0, 100],
    "num_inference_steps": 100,
    "content_weight": 1e-3,
    "output_dir": "models/emoji",
    "augmentation": ["double", "retrieval", "synonym"],
    "use_content_loss": true,
    "use_text_inversion": false,
    "use_sparse_update": true,
    "seed": 99999997,
    "pretrained_name": "CompVis/stable-diffusion-v1-4",    
    "image_size": 256,
    "checkpoints": [50, 100, 200, 400],
    "clip_similars": "similars/emoji-similars.pt",
    "gradient_accumulation_steps": 4
}